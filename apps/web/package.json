{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node scripts/setup-prisma.js && ./scripts/prepare-prod-build.sh && next build",
    "build:local": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "type-check": "tsc --noEmit",
    "db:push": "node scripts/setup-prisma.js && prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "node scripts/setup-prisma.js && prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:preview": "tsx prisma/seed.preview.ts",
    "db:seed:production": "tsx prisma/seed.production.ts",
    "db:reset": "prisma migrate reset",
    "db:test-login": "tsx prisma/test-login.ts",
    "docker:up": "docker-compose up -d postgres",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d postgres",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:shell": "docker-compose exec postgres psql -U postgres -d holiday_aggregator",
    "postinstall": "node scripts/setup-prisma.js && prisma generate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.13.0",
    "@react-email/components": "^0.4.0",
    "@react-email/render": "^1.1.4",
    "@stripe/stripe-js": "^7.8.0",
    "@t3-oss/env-nextjs": "^0.6.0",
    "@tanstack/react-query": "^5.84.1",
    "@trpc/client": "^11.4.3",
    "@trpc/next": "^11.4.3",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.4.3",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.537.0",
    "micro": "^10.0.1",
    "next": "^15.4.5",
    "next-auth": "^4.24.11",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-email": "^4.2.7",
    "resend": "^4.8.0",
    "stripe": "^18.4.0",
    "superjson": "^2.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^8.44.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.5",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "license-checker": "^25.0.1",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.1",
    "prisma": "^6.13.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "whatwg-fetch": "^3.6.20"
  },
  "ct3aMetadata": {
    "initVersion": "7.20.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}