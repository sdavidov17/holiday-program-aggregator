# Copy this to .env and fill in your actual credentials

# Google OAuth (get from https://console.cloud.google.com/)
GOOGLE_CLIENT_ID="your-google-client-id-here"
GOOGLE_CLIENT_SECRET="your-google-client-secret-here"

# Apple OAuth (get from https://developer.apple.com/)
APPLE_ID="your-apple-service-id-here"
APPLE_SECRET="your-apple-private-key-here"

# Discord OAuth (get from https://discord.com/developers/applications)
DISCORD_CLIENT_ID="your-discord-client-id-here"
DISCORD_CLIENT_SECRET="your-discord-client-secret-here"

# Encryption key for PII data
ENCRYPTION_KEY="your-32-character-encryption-key-here"